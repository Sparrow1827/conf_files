#! /bin/bash/

cd && yay -Syyu --noconfirm && yay -S --noconfirm i3status cava htop dmenu i3-gaps rofi nautilus gedit zsh evince vlc maim gthumb cheese rxvt-unicode feh imagemagick python-pip python-pywal xbanish google-chrome spotify gtk-engine-murrine sassc gtk3 pavucontrol-qt youtube-dl gtop otf-overpass librewolf-bin torbrowser-launcher kitty picom neovim adobe-source-code-pro-fonts && cd && git clone https://github.com/vinceliuice/Tela-circle-icon-theme.git && cd && cd Tela-circle-icon-theme && ./install.sh -c blue && cd && git clone https://github.com/vinceliuice/Graphite-gtk-theme.git && cd && cd Graphite-gtk-theme && ./install.sh --tweaks rimless && cd && cd Graphite-gtk-theme && cd other && cd grub2 && sudo ./install.sh && sh -c "$(curl -fsSL https://raw.githubusercontent.com/robbyrussell/oh-my-zsh/master/tools/install.sh)" &&  git clone https://github.com/zsh-users/zsh-syntax-highlighting.git $ZSH_CUSTOM/plugins/zsh-syntax-highlighting &&  git clone https://github.com/romkatv/powerlevel10k.git $ZSH_CUSTOM/themes/powerlevel10k && cd && cd conf_files && cp -r ~/conf_files/.config/ ~/ && cd && cd conf_files && cd Wal && mv bg1.jpg ~/Pictures yay -Syyu --noconfirm 
